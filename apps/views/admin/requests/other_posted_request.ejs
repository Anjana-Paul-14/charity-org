    							              <th>TITLE</th>
                                <th>REQUESTS</th>
                                <th>PAYMENT</th>
                                
                               
                            </tr>
                        </thead>
                        <tbody>
                            <% if (data && data.request){%>
                                <% for(var i = 0; i < data.request.length; i++){%>
                                <tr>
                                    <td> <%= data.request[i].title %> </td>
                                    <td> <%= data.request[i].need %> </td>
                                    <td> <% if(data.request[i].payment=='y'){ %>Paid<% }else{ %> Not Paid
                                    <% } %></td>
                                    <td><a href="fund_req_other_samithy/<%= data.request[i].r_id%>">view details</a></td>

                                    <td><a href ="upload_fund_slip/<%= data.request[i].r_id%>">Upload</a></td> 
                                   
                                </tr>
                                <%}%>
                            <%}%>
                        </tbody>
                    </table>
                    <INPUT TYPE="button" VALUE="Back" onClick="history.back()" a href="admin/index" >
                    </form>
                </div>
                <!-- /.table-responsive -->
            </div>
