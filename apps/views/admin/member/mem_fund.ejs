
  <%- include('../layout/header') %>
<%- include('../layout/side') %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1><center>Member Fund Deposit Form</center></h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">General Form</li>
            </ol>
          </div>
        </div>
     


    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <!-- left column -->
          <div class="col-md-12">
            <!-- general form elements -->
            <div class="card card-primary">
              <div class="card-header">
                <h3 class="card-title">Fund Deposit Form</h3>


              </div>
              <!-- /.card-header -->
              <!-- form start -->
  
                
              <form role="form" action="/admin/add_fund" method="post">
                
                


                    <div class="card-body">
  

                  <div class="form-group">
                  <label class="col-sm-2 col-sm-2 control-label">Member ID</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="memid" id="memid">
                     <% if(errors){ %>
                        <%= errors %>
                     <% } %>
                  </div>
                </div>

                  <div class="form-group">
                  <label class="col-sm-2 col-sm-2 control-label">Amount</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="fund">
                  </div>
                  </div
                  <div class="form-group">
                  <label class="col-sm-2 col-sm-2 control-label">Description</label>
                  <div class="col-sm-10">
                    <textarea type="text" class="form-control" name="description"></textarea>

                  </div>

                   <div class="form-group">
                  <label class="col-sm-2 col-sm-2 control-label">Deposit Date</label>
                  <div class="col-sm-10">
                    <input type="date" name="ddate" >
                  </div>
               </div>

                </div>
                <div class="card-footer">
                  <button type="submit" class="btn btn-primary">Submit</button>
                </div>
              </form>
        </div>

          </div>
        </div>
        <!-- /.row -->
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>

   </div><!-- /.container-fluid -->
    </section>

  <%- include('../layout/footer') %>