
<%- include('partials/f_header'); -%>


    <main role="main">
      <!-- Content -->
      <article>
        
        
        <div class="section background-white"> 
          <div class="line">
            <div class="margin">
              <div class="s-12 m-6 l-4">
                <div class="image-with-hover-overlay image-hover-zoom margin-bottom">
                 
         
                </div>  
              </div>
              <div class="s-12 m-6 l-4">





                <form name="contactForm" class="customform"  method="POST" action="/samithy_signup">
                 
                  <div class="line">
                    <div class="margin">
                      <div class="s-12 ">
                        <input name="email" value="" class="required email" placeholder="Your e-mail" title="Your e-mail" type="text" />
                        <p class="email-error form-error">Please enter your e-mail.</p>
                      </div>



                     
                    </div>
                         <div class="s-12"> 
                  <label  class="col-sm-5 col-sm-5 control-label">Samithi Name</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="first_name">
                  </div>
                  </div
                  <div class="s-12"> 
                  <label  class="col-sm-5 col-sm-5 control-label">Last name</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="last_name">
                  </div>
                </div>

             

                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Address</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="address">
                  </div>
                </div>

              

              <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Phone Number</label>
                  <div class="col-sm-10">
                    <input type="number" class="form-control" name="pno">
                  </div>
                </div>

                <div class="s-12"> 
                  <label  class="col-sm-5 col-sm-5 control-label">Email ID</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="email">
                  </div>
                </div>



                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Password</label>
                  <div class="col-sm-10">
                    <input type="password" class="form-control" name="password">
                  </div>
                </div>

                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">ward</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="ward">
                  </div>
                </div>

                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Panchayath</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="pan">
                  </div>
                </div>
                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Taluk</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="taluk">
                  </div>
                </div>


                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">District</label>
                  <div class="col-sm-10">
                    
                        <select id="districts" name="dis">

                          <option value="1">Kasargod</option>
                          <option value="2">Kannur</option>
                          <option value="3">Calicut</option>
                          <option value="4">Wayanad</option>
                          <option value="5">Palakad</option>
                          <option value="6">Malapuram</option>
                          <option value="7">Thrissur</option>
                          <option value="8">Ernakulam</option>
                          <option value="9">Idukki</option>
                          <option value="10">Kottayam</option>
                          <option value="11">Alapuzha</option>
                          <option value="12">Kollam</option>
                          <option value="13">Pathanamthitta</option>
                          <option value="14">Trivandrum</option>
                         
                         </select>
                    
                  </div>
                </div>
                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">State</label>
                  <div class="col-sm-10">
                    
                        <select id="state" name="state">

                          <option value="1">kerala</option>
                          <option value="2">TN</option>
                          <option value="3">Karnataka</option>
                          <option value="4">AP</option>
                          <option value="5">Maharashtra</option>
                          <option value="6">Goa</option>
                          <option value="7">Punjab</option>
                          <option value="8">Rajasthan</option>
                          
                         
                         </select>
                    
                  </div>
                </div>
                
                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Pincode</label>
                  <div class="col-sm-10">
                    <input type="number" class="form-control" name="pin">
                  </div>
                </div>

                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Bank Account Number</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="bankno">
                  </div>
                </div>

                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Bank Account Holder Name</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="bankholdername">
                  </div>
                </div>
                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">IFSC CODE</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="bankifsc">
                  </div>
                </div>
                <div class="s-12"> 
                  <label class="col-sm-5 col-sm-5 control-label">Branch details</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="bankdetails">
                  </div>
                </div>

                  
                 <div class="s-12"><button class="s-12 submit-form button background-primary text-white" type="submit">Submit</button>

                  </div>




                </div>



                  </div>
                 
                 
                 
                </form>






  </div>
             
            
             
            
            </div>  
          </div>
        </div> 
      </article>
    </main>

